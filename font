#REPOCAD single stroke font

def A(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line((x) (y) ((h / 2) + x) hgt + y)
line((h + x) (y) ((h / 2) + x) hgt + y)
line(((h / 1.3) + x) ((hgt / 2) + y) ((h / 4) + x) (hgt / 2) + y)
}

def B(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
def mh2 = h / 0.65
line(x 0 + y 0 + x hgt + y)
line(x hgt + y mh + x hgt + y)
line(x mh2 + y mh + x mh2 + y)
line(x y mh + x y)
arc((mh + x) (h / 1.3) + y h / 1.3 -1.55 1.55)
arc((mh + x) (h / 0.44) + y h / 1.4 -1.55 1.55)
}

def C(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
line((x) ((hgt * 0.35) + y) (x) (hgt * 0.65) + y)
arc((h + x) (h / 0.5) + y h -3.14 -1.3)
arc(h + x h + y h 1.3 3.14)
}

def D(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
line(x y x hgt + y)
line(x hgt + y mh + x hgt + y)
line(x y mh + x y)
line((mh + x + h) ((hgt * 0.35) + y) (mh + x + h) (hgt * 0.65) + y)
arc((mh + x) (h / 0.5) + y h -1.55 0)
arc(mh + x h + y h 0 1.55)
}

def E(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
line(x y x hgt + y)
line(x hgt + y h + x hgt + y)
line((x) (hgt / 2) + (y) (mh + x) (hgt / 2) + y)
line(x y h + x y)
}

def F(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
line(x 0 + y 0 + x hgt + y)
line(x hgt + y h + x hgt + y)
line((x) (hgt / 2) + (y) (mh + x) (hgt / 2) + y)
}

def G(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.8
line((x) ((hgt / 1.4) + y) (x) r + y)
arc((r + x) (hgt / 1.4) + y r -3.14 -0.4)
arc(r + x r + y r 0 3.14)
line((x + r * 2) ((hgt / 2) + y) (x + r * 2) r + y)
line((x + r * 2) ((hgt / 2) + y) (r + x) (hgt / 2) + y)
}

def H(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line(x y x hgt + y)
line(h + x y h + x hgt + y)
line((x) (hgt / 2) + (y) (h + x) (hgt / 2) + y)
}

def I(x as Number y as Number hgt as Number) = {
def h = hgt / 2
def mh = 1.4 * h
line(((mh / 2) + x) (y) (mh / 2) + x hgt + y)
}

def J(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.8
arc(r + x r + y r 0 3.14)
line((x + r * 2) (hgt + y) (x + r * 2) r + y)
}

def K(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line(x y x hgt + y)
line((x) (hgt / 2) + (y) (h + x) hgt + y)
line((x) (hgt / 2) + (y) (h + x) y)
}

def L(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
line(x y x hgt + y)
line(x y h + x y)
}

def M(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line(x y x hgt + y)
line(h + x y h + x hgt + y)
line((x) hgt + (y) ((h / 2) + x) (hgt / 2) + y)
line((h + x) hgt + (y) ((h / 2) + x) (hgt / 2) + y)
}

def N(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line(x y x hgt + y)
line(h + x y h + x hgt + y)
line((x) hgt + (y) (h + x) y)
}

def O(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
line((x) ((hgt / 1.3) + y) (x) r + y)
arc((r + x) (hgt / 1.28) + y r -3.14 0)
arc(r + x r + y r 0 3.14)
line((x + r * 2) ((hgt / 1.3) + y) (x + r * 2) r + y)
}

def P(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
def mh2 = h / 0.65
line(x 0 + y 0 + x hgt + y)
line(x hgt + y mh + x hgt + y)
line(x mh2 + y mh + x mh2 + y)
arc((mh + x) (h / 0.44) + y h / 1.4 -1.55 1.55)
}

def Q(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.8
line((x) ((hgt / 1.4) + y) (x) r + y)
arc((r + x) (hgt / 1.4) + y r -3.14 0)
arc(r + x r + y r 0 3.14)
line((x + r * 2) ((hgt / 1.4) + y) (x + r * 2) r + y)
line((h + x) ((h / 1.2) + y) ((h * 1.4) + x) y) 
}


def R(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
def mh2 = h / 0.65
line(x 0 + y 0 + x hgt + y)
line(x hgt + y mh + x hgt + y)
line(x mh2 + y mh + x mh2 + y)
line((mh + x) (mh2 + y) ((h * 1.2) + x) y)
arc((mh + x) (h / 0.44) + y h / 1.4 -1.55 1.55)
}

def S(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
arc((r + x) (hgt / 1.3) + y r -3.94 0)
arc(r + x r + y r -1 3.4)
line(((h / 4.5) + x) ((h * 1.8) + y) ((h * 1.1) + x) ((h * 1.3) + y))
}

def T(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 1.4 * h
line(((mh / 2) + x) (y) (mh / 2) + x hgt + y)
line(x hgt + y mh + x hgt + y)
}

def U(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.8
line((x) (hgt + y) (x) r + y)
arc(r + x r + y r 0 3.14)
line((x + r * 2) (hgt + y) (x + r * 2) r + y)
}

def V(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line((x) hgt + (y) ((h / 2) + x) y)
line((h + x) hgt + (y) ((h / 2) + x) y)
}
def W(x as Number y as Number hgt as Number) = {
def left = hgt / 6
def mid = hgt / 3
def right = hgt / 2
line(x hgt + y left + x y)
line(mid + x  hgt + y left + x y)
line(mid + x hgt + y right + x y)
line((hgt / 1.5) + x hgt + y right + x y)
}

def X(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line((x) hgt + (y)  h + x y)
line((h + x) hgt + (y) x y)
}

def Y(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line((x) hgt + (y) ((h / 2) + x) (hgt / 2) + y)
line((h + x) hgt + (y) ((h / 2) + x) (hgt / 2) + y)
line(((h / 2) + x) (y) ((h / 2) + x)(hgt / 2) + y)
}

def Z(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
line(x y x + h hgt + y)
line(x hgt + y h + x hgt + y)
line(x y h + x y)
}

def one(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line(((h / 2) + x) (y) (h / 2) + x hgt + y)
line(((h / 2) + x) hgt + (y) ((h / 5) + x) (hgt / 1.3) + y)
line(((h / 4) + x) (y) (h / 1.3) + x y)
}

def two(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
arc((r + x) (hgt / 1.3) + y r -3.2 0.60)
line(((h * 1.32) + x) ((h * 1.97) + y) ((h / 8) + x) ((h * 0.4) + y))
arc(r + x y r 3.14 3.8)
line(x y x + r * 2 y)
}

def three(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
def mh2 = h / 0.65
arc((mh + x) (h / 1.3) + y h / 1.3 -1.55 2.75)
arc((mh + x) (h / 0.44) + y h / 1.4 -2.35 1.75)
}

def four(x as Number y as Number hgt as Number) = {
def h = hgt / 2
line((h + x) (y) h + x hgt + y)
line((h + x) hgt + (y) ((h / 5) + x) h + y)
line(((h / 4) + x) (h + y) h + x h + y)
}

def five(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 0.5 * h
def mh2 = h / 0.65
arc((mh + x) (h / 1.3) + y h / 1.3 -1.57 1.57)
line(x y mh + x y)
line(x mh2 + y mh + x mh2 + y)
line(x hgt + y h + x hgt + y)
line((x) (hgt + y) (x) (hgt / 2) + y)
}

def six(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
line((x) ((hgt / 1.3) + y) (x) r + y)
arc((r + x) (hgt / 1.28) + y r -3.14 0)
arc(r + x r + y r 1.57 3.14)
arc(((h * 0.7) + x) (h / 1.3) + y h / 1.3 -2.3 1.87)
}

def seven(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def mh = 1.4 * h
line(x hgt + y mh + x hgt + y)
arc((4.9 * h) + x y 4.5 * h 3.14 3.78)
arc(((mh / 2) + x) y + hgt 0.224 * hgt 0 0.5)
}

def eight(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
circle((r + x) (hgt / 1.28) + y r)
circle(((h * 0.7) + x) (h / 1.2) + y h / 1.22)
}

def nine(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
line(((hgt / 2.15) + x) ((hgt / 1.3) + y) ((hgt / 2.15) + x) r + y)
arc((r + x) (hgt / 1.28) + y r 0.7 6.28)
arc(r + x r + y r 0 3.14)
}

def zero(x as Number y as Number hgt as Number) = {
def h = hgt / 3
def r = h * 0.7
line((x) ((hgt / 1.3) + y) (x) r + y)
arc((r + x) (hgt / 1.28) + y r -3.14 0)
arc(r + x r + y r 0 3.14)
line((x + r * 2) ((hgt / 1.3) + y) (x + r * 2) r + y)
}

