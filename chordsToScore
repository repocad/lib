xG = -100
yG = 100

function score(x y) {
 
  xMin = x - 6
  xMax = x + 6
  yMin = y - 16
  yMax = y + 12
  line(xMin y + 12.5 xMax y + 12.5) 
 
  for i <- -2 to 3 {
     y2 = y + i * 6
     x2 = x + i * 3 
      line(xMin y2 xMax y2)
      line(x2 yMin x2 yMax)
  }
}

function a(x y){
  xG = xG + 20
  if (xG > 100) yG = yG + 20
  score(xG yG)
  circle(x y 2)
}


a(0 0)