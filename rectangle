def square(neg as Number pos as Number) = {
  line(neg neg neg pos)
  line(neg pos pos pos)
  line(pos pos pos neg)
  line(pos neg neg neg)
}

#example:
repeat -15 to 50 using i {
  def a = i + 1
  def b = i - 1
  square(a * 5 a * 10)
  square(a * -5 a * 10)
}

