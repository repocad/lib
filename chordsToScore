function score(x y) {
 
  xMin = x - 6
  xMax = x + 6
  yMin = y - 16
  yMax = y + 12
  line(xMin y + 6.5 xMax y + 6.5) 
 
  for i <- -2 to 3 {
     y2 = y + i * 6
     x2 = x + i * 3 
      line(xMin y2 xMax y2)
      line(x2 yMin x2 yMax)
  }
}

function a(x y){
  score(x y)
  circle(x y 2)
}


a(0 0)