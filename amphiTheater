pi = 3.14
halfPi = pi / 2


# FIRST THIRD
for i <- 3 to 20 {
a = i * 7
arc 60 10 a halfPi pi / 1.4 
}

# SECOND THIRD
for i <- 3 to 20 {
a = i * 7
arc 60 10 a pi / 1.3 pi 
}

# LAST THIRD
for i <- 3 to 20 {
a = i * 7
arc 60 10 a pi * 1.05 pi * 1.275 
}



