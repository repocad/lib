function square(neg pos) {
  line(neg neg neg pos)
  line(neg pos pos pos)
  line(pos pos pos neg)
  line(pos neg neg neg)
}

#example:
for i <- -15 to 50 {
a = i + 1
b = i - 1
square(a * 1 a * 2)
square(b * -1 a * 2)
}

