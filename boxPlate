def plate(width as Int height as Int x as Int y as Int radius as Int) =  {
 
  #boundary
  line(x y x y + height)
  line(x y x + width y)
  line(x + width y x + width y + height)
  line(x y + height x + width y + height)

 #bottom and top holes
 def bottomTop(offset as Int width as Int) = {
  def offset = 40
  circle(x + offset y + 10 radius)
  def rightSide = x + width 
  circle(rightSide - offset  y + 10 radius) 
  }
  #bottom holes
  bottomTop(40 
  #top holes  

}

#call the function plate
plate(140 140 10 10 3)