import boxPlate

def makeBox(width as Int height as Int depth as Int) = {
  def partSpacing = 30

#FRONT
plate(width height 0 0 3)

#BACK
plate(width height 0 550 3)

#BOTTOM
plate(width depth 0 200 3)

#SIDE 1 
plate(height depth -200 200 3)

#SIDE 2
plate(height depth 200 200 3)

}

makeBox(160 140 300)
