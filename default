# Draw a line from (0 0) to (-50 -50)
line(0 0 -60 -60)

# Import the geometry2d script
import geometry2d

# Use the rectangle function from geometry2d
# to draw a rectangle from (0 0 ) to (50 50 )
rectangle(0 0 -60 -60)

#loop five times, offsetting a line each time:
repeat 0 to 5 using i {
  def offset = i * 10
  line(0 offset -60 offset)
}